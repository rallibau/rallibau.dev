<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.inetum.livetooling.lista.tag.domain.Tag" table="tags">
        <composite-id name="id" class="com.inetum.livetooling.lista.tag.domain.TagId" access="field">
            <key-property column="id" name="value" length="36" access="field" />
        </composite-id>

        <component name="numero" class="com.inetum.livetooling.lista.tag.domain.TagNumero" access="field">
            <property name="value" column="numero" type="string" access="field" />
        </component>

        <component name="clienteId" class="com.inetum.livetooling.lista.tag.domain.TagClienteId" access="field">
            <property name="value" column="clienteId" type="string" access="field" />
        </component>

        <component name="tagOperador" class="com.inetum.livetooling.lista.tag.domain.TagOperador" access="field">
            <property name="value" column="tagoperador" type="integer" access="field" />
        </component>

        <component name="tagTipo" class="com.inetum.livetooling.lista.tag.domain.TagTipo" access="field">
            <property name="value" column="tagtipo" type="integer" access="field" />
        </component>

        <component name="tagCategoria" class="com.inetum.livetooling.lista.tag.domain.TagCategoria" access="field">
            <property name="value" column="tagcategoria" type="string" access="field" />
        </component>

        <component name="tagStatus" class="com.inetum.livetooling.lista.tag.domain.TagStatus" access="field">
            <property name="value" column="tagstatus" type="integer" access="field" />
        </component>

        <component name="tagHoraActualizacion" class="com.inetum.livetooling.lista.tag.domain.TagHoraActualizacion" access="field">
            <property name="value" column="taghoraactualizacion" type="string" access="field" />
        </component>

        <component name="tagFechaActualizacion" class="com.inetum.livetooling.lista.tag.domain.TagFechaActualizacion" access="field">
            <property name="value" column="tagfechaactualizacion" type="string" access="field" />
        </component>

        <component name="tagSaldo" class="com.inetum.livetooling.lista.tag.domain.TagSaldo" access="field">
            <property name="value" column="tagsaldo" type="string" access="field" />
        </component>
    </class>
</hibernate-mapping>
