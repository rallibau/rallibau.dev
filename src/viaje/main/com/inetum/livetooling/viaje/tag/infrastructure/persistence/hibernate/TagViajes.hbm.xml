<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.inetum.livetooling.viaje.tag.domain.TagViajes" table="viajes_tags">
        <composite-id name="id" class="com.inetum.livetooling.viaje.tag.domain.TagViajesId" access="field">
            <key-property column="id" name="value" length="36" access="field" />
        </composite-id>

        <component name="numero" class="com.inetum.livetooling.viaje.tag.domain.TagViajesNumero" access="field">
            <property name="value" column="numero" type="string" access="field" />
        </component>

        <property name="clienteId" column="clienteId" type="string" access="field" />
    </class>
</hibernate-mapping>
