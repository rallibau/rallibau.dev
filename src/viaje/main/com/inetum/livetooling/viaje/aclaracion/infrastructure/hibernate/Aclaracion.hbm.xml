<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.inetum.livetooling.viaje.aclaracion.domain.Aclaracion" table="aclaracion">
        <composite-id name="id" class="com.inetum.livetooling.viaje.aclaracion.domain.AclaracionId" access="field">
            <key-property column="id" name="value" length="36" access="field" />
        </composite-id>

        <component name="estado" class="com.inetum.livetooling.viaje.aclaracion.domain.AclaracionEstado" access="field">
            <property name="value" column="estado" type="string" access="field" />
        </component>


    </class>
</hibernate-mapping>
